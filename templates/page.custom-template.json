    "ap_section_our_story_tjtRb3": {
      "type": "ap-section-our-story",
      "settings": {
        "title": "Our Story",
        "desc": "<p>Boston International (BI) , a woman-owned, design-driven company, has led the gift industry since 1986. Starting as an import company, we became the exclusive distributor of Ideal Home Range® (IHR) in 2001, offering high-quality paper linen and tabletop accessories from Germany. We proudly partner with prestigious brands and artists to provide superior-quality products that celebrate the home and reflect our passion for entertaining.<\/p>",
        "button_white": false,
        "btn_text": "Join Our Team",
        "btn_link": "shopify:\/\/pages\/careers",
        "padding_top": 90,
        "padding_bottom": 60,
        "bg_color": "#f4eee9",
        "text_color": "#000000",
        "blk_bg_color": "#a2ab94",
        "blk_text_color": "#ffffff"
      }
